<!DOCTYPE html>
<html>
<head>
  <title>Movieplex</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<%= render 'header' %>
<div id="wrapper">

  <div id="container">
    <section id="sidebar">
		<%= render 'sidebar' %>
	</section>
    <section id="content">
      <%= yield %>
    </section>
  </div>

</div>
		
	<%= render 'footer' %>

</body>
</html>
